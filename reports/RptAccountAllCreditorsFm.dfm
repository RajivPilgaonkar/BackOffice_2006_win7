  object AllCreditorsSds: TSQLDataSet
    CommandText = 
      'SELECT T.yearref, T.voucherno, V.voucherdate, V.description, T.B' +
      'illedAmt, T.paidamt, T.exchrate,'#13#10'       V.addressbook_id, A.add' +
      'ressbook_id as a_addressbook_id, '#13#10'       COALESCE(A.organisatio' +
      'n,'#39#39') + '#39', '#39' + COALESCE(A.City,'#39#39')  as organisation, '#13#10'       (c' +
      'ase when T.BilledAmt>0 then T.BilledAmt  else expamt end) as Exp' +
      'Due,'#13#10'       (case when T.BilledAmt>0 then (T.BilledAmt-T.paidam' +
      't) else (T.expamt-T.paidamt) end) as Balance,'#13#10'       (case when' +
      ' V.currencies_id <> 13 then 1 else 0 end) as USD,'#13#10'       (case ' +
      'when V.currencies_id <> 13 then  '#13#10'             (case when T.Bil' +
      'ledAmt>0 then (T.BilledAmt-T.paidamt) else (T.expamt-T.paidamt) ' +
      'end)'#13#10'             else 0 end) as USD_Balance,'#13#10'       ((case wh' +
      'en T.BilledAmt>0 then T.BilledAmt  else expamt end) * V.exchange' +
      'rate) as Exc_ExpDue,'#13#10'       ((case when T.BilledAmt>0 then (T.B' +
      'illedAmt-T.paidamt) else (T.expamt-T.paidamt) end) * V.exchanger' +
      'ate) as Exc_Balance'#13#10'FROM tmpsundrycreditors T'#13#10'     LEFT JOIN v' +
      'ouchers V ON T.vouchers_id=V.vouchers_id  '#13#10'     LEFT JOIN fn_ad' +
      'dressbook(3,'#39#39') A ON V.addressbook_id=A.addressbook_id '#13#10'WHERE R' +
      'upeeBalance <> 0'#13#10'AND V.IncludeInBalanceSheet = 1'#13#10
    MaxBlobSize = -1
    Params = <>
    SQLConnection = BackOfficeDataModule.SQLConnection
    Left = 248
    Top = 16
  end
